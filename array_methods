//array methods

var array1 = [1, 2, 3, 4, 5, 7, 8, 9, 10];
var array2 = [11, 12, 13, 14, 15, 17, 18, 19, 20];

array1.push(array2);
console.log(array1);

var stringArr = ["Red Deer", "Edmonton", "Calgary"];
stringArr.push("Lacombe", "Innisfail");
stringArr.pop();

console.log(stringArr);

var arrayJoin = ["oranges", "apples", "cherries"];

console.log(arrayJoin.join(';'));

arrayJoin.filter((item) => item.length > 7);

console.log(arrayJoin);
